{
  "name": "oadtz/checkout",
  "description": "Checkout library using some payment gateway",
  "license": "MIT",
  "keywords": [
    "Oadtz",
    "Checkout"
  ],
  "authors": [
    {
      "name": "Thanapat Pirmphol",
      "email": "t.pirmphol@gmail.com",
      "homepage": "https://www.oadtz.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/config": "5.2.*",
    "adyen/php-api-library": "^1.5",
    "braintree/braintree_php": "^3.36"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.*",
    "symfony/var-dumper": "2.*",
    "fzaninotto/faker": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Oadtz\\Checkout\\": "src",
      "Oadtz\\Library\\": "src/library"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Oadtz\\Checkout\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "dev"
    }
  },
  "minimum-stability": "stable"
}
